<div class="subnav">
  <%= link_to "Dashboard", user_dashboard_path, class: "subtitle" %>
  <%= link_to "Inbox", conversations_path, class: "subtitle active" %>
</div>

<p>Page index</p>


<% @conversations.each do |conversation| %>
  <p>
    <%= link_to conversation_path(conversation) do %>
      <%= conversation.cellar.user.first_name %> <%= conversation.cellar.user.last_name %><br>
      <%= conversation.messages.last.content %>
    <% end %>
  </p>
<% end %>



